########################################
###  ParaDiS control parameter file  ###
########################################

#  This example is an 8 cpu simulation of an HCP (beryllium) crystal
#  under constant stress.  Simulation uses isotropic elasticity for
#  all forces and FMM for far-field forces.
#
#  Output generated by this example will be placed under the directory
#  specified by the <dirname> value below and includes:
#
#      - restart files
#      - flux decomposition
#      - various properties files
#      - gnuplot visualization files
#      - VisIt visualization files

dirname   = "Be_results"

numXdoms  = 2
numYdoms  = 2
numZdoms  = 2

numXcells = 8
numYcells = 8
numZcells = 8

#  FMM setup

fmEnabled       = 1
fmMPOrder       = 2
fmTaylorOrder   = 5
fmCorrectionTbl = "../inputs/fm-ctab.Beryllium.m2.t5.dat"

#  Load balancing setup

DLBfreq    = 1
decompType = 2

#  Discretization controls

remeshRule = 2
maxSeg     = 300

#  Timestep controls

maxstep            = 1000
timestepIntegrator = "trapezoid"
nextDT             = 1.0e-11
rTol               = 0.75

#  Material properties: Beryllium at ambient conditions
#
# C11 = 291 GPa
# C12 = 19 GPa
# C13 = 24 GPa
# C33 = 342 GPa
# C44 = 150 GPa

mobilityLaw  = "HCP_linear"

burgMag      = 2.2814e-10
pois         = 0.06439      # Voight average
shearModulus = 144.33333e9  # Voight average
rc           = 3.0

cOVERa       = 1.5680 
HCPEcoreA    = 18.06977e9  # for rc=3.0
HCPEcoreC    = 21.70031e9  # for rc=3.0
HCPEcoreCpA  = 7.18520e9   # for rc=3.0

# Drag coefficients for beryllium

HCP_A_Basal_EdgeDrag           = 1.18434387898773e-04
HCP_A_Basal_ScrewDrag          = 1.65421659553579e-04

HCP_A_Prismatic_EdgeDrag       = 5.69513923115224e-05
HCP_A_Prismatic_ScrewDrag      = 7.15096201802962e-05

HCP_A_1stPyramidal_EdgeDrag    = 7.28268834977848e-05
HCP_A_1stPyramidal_ScrewDrag   = 1.05154866364405e-04

HCP_A_2ndPyramidal_EdgeDrag    = 7.28268834977848e-05
HCP_A_2ndPyramidal_ScrewDrag   = 1.05154866364405e-04

# From polyxtal literature, values are assumed 20 times basal edge

HCP_CpA_Prismatic_EdgeDrag     = 2.36868775797546e-03
HCP_CpA_Prismatic_ScrewDrag    = 2.36868775797546e-03

HCP_CpA_1stPyramidal_EdgeDrag  = 2.36868775797546e-03
HCP_CpA_1stPyramidal_ScrewDrag = 2.36868775797546e-03

HCP_CpA_2ndPyramidal_EdgeDrag  = 2.36868775797546e-03
HCP_CpA_2ndPyramidal_ScrewDrag = 2.36868775797546e-03

HCP_C_Prismatic_EdgeDrag       = 2.36868775797546e-03
HCP_C_Prismatic_ScrewDrag      = 2.36868775797546e-03

HCP_Sessile_EdgeDrag           = 23.6868    # 10000 * pyramidal
HCP_Sessile_ScrewDrag          = 23.6868    # 10000 * pyramidal

HCP_LineDrag                   = 5.e-6      # ~10 times lowest

#  Loading conditions

loadType      = 0  # constant stress
appliedStress = [
    0.000000e+00
    0.000000e+00
    0.000000e+00
    0.000000e+00
    3.000000e+08
    0.000000e+00   
]

#  Output selections

savecn          = 1
savecnfreq      = 200
saveprop        = 1
savepropfreq    = 100
fluxfile        = 1
fluxfreq        = 200
gnuplot         = 1
gnuplotfreq     = 200
writeVisit      = 1
writeVisitFreq  = 200

winDefaultsFile =  "../inputs/paradis.xdefaults"  

