#
#  This example is an 8 cpu simulation of the rhombohedral phase
#  of a Vanadium crystal using a non-linear mobility module and
#  restricting motion to the dislocation segments' assigned glide
#  planes.
#  
#  Output generated by this example will be placed under the directory
#  specified by the <dirname> value below and includes:
#
#      - restart files
#      - flux decomposition
#      - various properties files
#      - gnuplot visualization files
#      - nodal velocity files
#

#
#  Simulation cell and processor setup
#  
numXdoms =   2
numYdoms =   2  
numZdoms =   2  
numXcells =   4  
numYcells =   4  
numZcells =   4  
#
#  Load balancing setup
#
decompType =   2  
DLBfreq =   3
#
#  Simulation time and timestepping controls
#  
nextDT = 1.0e-12
maxstep =   20000
timestepIntegrator =   "trapezoid"  
rTol =  3.0 # 1.250000e+00  
#
#  Discretization controls
#  
maxSeg =   4.000000e+02  
minSeg =   8.000000e+01  
remeshRule =   2  
#
#  Fast Multipole Method controls
#  
fmEnabled =   1  
fmMPOrder =   2  
fmTaylorOrder =   5  
fmCorrectionTbl =   "inputs/fm-ctab.R-V.150GPa.m2.t5.dat"  
#
#  Loading conditions
#  
TempK =   6.0e+02 # 3.000000e+02  
pressure = 1.5e+11 # 150 GPa #
loadType =   1  
eRate =   1.000000e+05  
indxErate =   0  
edotdir = [ 0.  0.  1 ]
# appliedStress = [ 0 0 3e9 0 0 0 ]
#
#  Material and mobility parameters
#  
mobilityLaw =   "Rhombohedral_Va_nl_planar"  
enforceGlidePlanes = 1
enableCrossSlip = 1
shearModulus =   99.6e9
pois =   0.43
burgMag =   2.511473671e-10  
rc =   5.000000e+00  
Ecore =   5.502262e+10  
MobGlide =   1.000000e+02  
#
#  I/O controls and parameters
#  
dirname =   "tests/rhomboVa_results"
savecn =   1  
savecnfreq =   100
saveprop =   1  
savepropfreq =   50  
savetimers =   0
gnuplot = 1
gnuplotfreq = 100
fluxfile =   1  
fluxfreq =   100
fluxcounter =   0  
velfile = 1
velfilefreq = 100
