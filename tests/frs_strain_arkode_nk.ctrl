#
#  This example is a single cpu simulation of a simple frank read source.
#
#  Output generated by this example will be placed under the directory
#  specified by the <dirname> value and includes:
#    restart files
#    gnuplot visualization files
#    povray visualization files
#    various properties files

dirname   = "frs_strain_arkode_nk.rslts"

numXdoms  = 2  
numYdoms  = 2  
numZdoms  = 2  

numXcells = 4  
numYcells = 4  
numZcells = 4  

fmEnabled       = 0
fmMPOrder       = 2
fmTaylorOrder   = 5
fmCorrectionTbl = "../inputs/fm-ctab.Ta.600K.0GPa.m2.t5.dat"
Rijmfile        = "../inputs/Rijm.cube.out"  
RijmPBCfile     = "../inputs/RijmPBC.cube.out"  
winDefaultsFile = "../inputs/paradis.xdefaults"

maxstep         = 2500
remeshRule      = 2  
minSeg          = 100
maxSeg          = 1000
rTol            = 1.0

timestepIntegrator = "ARKODE"

#timeEnd = 1.0e-5 

ARKODE_IRKtable          = 100
ARKODE_FPsolver          = 0
ARKODE_MaxNonLinIters    = 5
ARKODE_MaxLinIters       = 5
ARKODE_FPaccel           = 0
ARKODE_AdaptivityMethod  = 2
ARKODE_PredictorMethod   = 0
ARKODE_MaxConvFailGrowth = 0.5
ARKODE_NonLinCoef        = 1.0
ARKODE_LinCoef           = 0.1

rc          = 5.0  
MobScrew    = 1.000000e+01  
MobEdge     = 1.000000e+01  
MobClimb    = 1.000000e-08  
mobilityLaw = "BCC_0"  

loadType    = 1  
eRate       = 1
indxErate   = 2  

edotdir = [
  0.0
  0.0
  1.0
  ]

appliedStress = [
  0.000000e+00
  0.000000e+00
  5.000000e+08
  0.000000e+00
  0.000000e+00
  0.000000e+00
  ]

savetimers     = 1  
savetimersfreq = 50
savecn         = 1  
savecnfreq     = 100  
savecncounter  = 0  
gnuplot        = 1  
gnuplotfreq    = 100  
gnuplotcounter = 0  
povray         = 1  
povrayfreq     = 250  
povraycounter  = 0  
saveprop       = 1  
savepropfreq   = 50
